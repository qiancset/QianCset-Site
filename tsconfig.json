{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/UI/*": [
        "app/UI/*"
      ],
      "@/locales/*": [
        "locales/*"
      ],
      "@/Providers/*": [
        "Providers/*"
      ],
      "@/public/*": [
        "public/*"
      ],
      "@/Component/*": [
        "app/(Apps)/Component/*"
      ],
      "@/CSS/*": [
        "app/(Apps)/CSS/*"
      ],
      "@/components/*": [
        "pages/_About/components/*"
      ],
      "@/css/*": [
        "pages/_About/css/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mdx",
    "app/middleware.js"
, "theme.config.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
