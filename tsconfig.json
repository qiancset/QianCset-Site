{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/config/*": [
        "app/config/*"
      ],
      "@/gui/*": [
        "gui/*"
      ],
      "@/locales/*": [
        "locales/*"
      ],
      "@/public/*": [
        "public/*"
      ],
      "@/Data/*":[
        "app/(Apps)/(Data)/*"
      ],
      "@/Component/*": [
        "app/(Apps)/Component/*"
      ],
      "@/CSS/*": [
        "app/(Apps)/CSS/*"
      ],
      "@/component/*": [
        "app/(www)/component/*"
      ],
      "@/css/*": [
        "app/(www)/css/*"
      ],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.mdx",
    "app/middleware.js"
, "theme.config.tsx", "Providers/Rainbowkit.jsx", "Providers/Themes.jsx", "app/(Apps)/(Nav)/(Router)/Dappbrowser/Uinswap/page.jsx", "Providers/Antd.jsx", "Providers/Providers.jsx", "Providers/lib/AntdRegistry.tsx", "gui/ModeThemes/ModeThemes.jsx", "pages/_app.jsx", "gui/LocalesButton/LocaleContext.jsx", "gui/LocalesButton/Locales.jsx", "app/(www)/(Router)/layout.tsx", "app/(Apps)/Component/AppsHeader/AppsHeader.jsx", "mdx-components.js", "app/(www)/(Router)/docs/page.jsx", "app/(www)/components/Header/Header.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
